{
  "manifest_version": 3,
  "name": "My React App Extension",
  "version": "1.0",
  "description": "A Chrome extension to display my React app.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "favicon.ico"
  },
  {
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
  }  
}
