{
  "manifest_version": 3,
  "name": "Form.io Demo Extension",
  "version": "1.0",
  "description": "A Chrome extension to display Form.io forms",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo512.png"
    }
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo512.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline'"
  },
  
  "permissions": [],
  "host_permissions": []
}